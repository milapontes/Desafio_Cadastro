MENU_WOMEN = '.sf-menu > :nth-child(1)'
BTN_VESTIDO = ':nth-child(2) > .product-container > .left-block > .product-image-container > .product_img_link > .replace-2x'
BTN_ADC_CARRINHO = 'add_to_cart"'


Cypress.Commands.add('clicarMenu', () =>{
    cy.get(MENU_WOMEN).should('be.visible')
} )

Cypress.Commands.add('adicionarProduto', () =>{
   cy.get(MENU_WOMEN).click()
   cy.get(BTN_VESTIDO).click()
   cy.get(BTN_ADC_CARRINHO).click()
} )